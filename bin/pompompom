#!/usr/bin/env ruby

if File.symlink?(__FILE__)
  $: << File.expand_path('../../lib', File.readlink(__FILE__))
else
  $: << File.expand_path('../../lib', __FILE__)
end

require 'rubygems'

gem 'hpricot'
gem 'immutable_struct'

require 'pompompom'
require 'pompompom/cli'

exit PomPomPom::Cli.new(STDIN, STDOUT, STDERR).run!(*ARGV)